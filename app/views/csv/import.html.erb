<p>Find me in app/views/csv/import.html.erb</p>
<%= form_for :upload, :html => { :multipart => true } do |f| -%>
  <div class="field">
    <%= f.label :lead_source %><br />
    <%= f.text_field :lead_source %>
  </div>
  <div class="field">
    <%= f.label :response_type %><br />
    <%= f.text_field :response_type %>
  </div>
  
  <%= f.file_field :csv %>
  <%= f.submit "Upload", :disable_with => 'Uploading...' %>
<% end -%>